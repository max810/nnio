{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "inputs": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "params": {
      "type": "object",
      "properties": {
        "input_shape": {
          "type": "array",
          "items": {
            "type": "integer"
          }
        },
        "units": {
          "type": "integer"
        },
        "kernel_regularizer": {
          "type": "object",
          "properties": {
            "regularization_type": {
              "type": "string"
            },
            "l1": {
              "type": "number",
              "enum": [
                1,
                2,
                3,
                4
              ]
            },
            "l2": {
              "type": "number",
              "minimum": 0.0
            }
          },
          "required": [
            "regularization_type",
            "l1",
            "l2"
          ]
        },
        "test_prop_obj": {
          "type": "object",
          "enum": [
            {
              "a": 1,
              "b": 2
            },
            {
              "a": 4,
              "b": 8
            }
          ]
        }
      },
      "required": [
        "input_shape"
      ]
    },
    "required": [
      "name",
      "type",
      "inputs",
      "params"
    ]
  }
}